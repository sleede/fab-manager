# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

/public/uploads
/public/assets

# Ignore application configurations
/config/application.yml
/config/database.yml

*.DS_Store
.idea

# PDF invoices
/invoices/*

.DS_Store

.vagrant
